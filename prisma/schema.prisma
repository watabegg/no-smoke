generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Smoking {
  id        String   @id @default(cuid())
  timestamp String 
}

model Cigarette {
  id        String   @id @default(cuid())
  brand     String
  tar       Int
  nicotine  Float
}
